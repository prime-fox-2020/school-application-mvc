<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Subject List</title>
</head>
<body>
    <h1>Subject List</h1>
    <a href="/" role="button">Back to Homepage</a>

    <% if(data.length) { %>
        <table border="1">
            <tr>
                <td>ID</td>
                <td>Nama Subject</td>
            </tr>
            <% data.forEach(item => { %>
                <tr>
                    <td><%= item.id%></td>
                    <td><%= item.subject_name %></td>
                </tr>
            <% }) %>
        </table>
    <% } %>
</body>
</html>