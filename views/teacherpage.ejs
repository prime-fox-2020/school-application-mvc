<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Teacher Pages</title>
</head>
<body>
    <h1>Teacher Page List</h1>
    <a href="/" role="button">Back to Homepage</a>

    <% if (data.length) {%>
        <table border="1">
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Gender</th>
            </tr>
            <% data.forEach(item =>{ %>
                <tr>
                    <td><%= item.id%></td>
                    <td><%= item.first_name%></td>
                    <td><%= item.last_name%></td>
                    <td><%= item.email%></td>
                    <td><%= item.gender%></td>
                </tr>
            <% }) %>
        </table>
    <% }%>
</body>
</html>