<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Students</title>
</head>
<body>
    <h1>Students Data</h1>
    <a href="/students/add">add students</a>

    <table border="1">
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Options</th>
        </tr>
        <% data.forEach(el => { %>
        <tr>
         <td><%= el.id %> </td>
         <td><%= el.first_name %> </td>
         <td><%= el.last_name %> </td>
         <td><%= el.email %> </td>
         <td><a href="students/<%= el.id %>/edit">EDIT</a> | <a href="students/<%= el.id %>/delete">DELETE</a></td>
        </tr>
        <% }) %>
    </table>
</body>
</html>